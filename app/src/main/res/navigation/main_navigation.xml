<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_add_event"
        android:name="com.maungedev.event.ui.add.choose_event.ChooseEventFragment"
        android:label="fragment_add_event"
        tools:layout="@layout/fragment_choose_event" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.maungedev.profile.ui.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.maungedev.event.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_insight"
        android:name="com.maungedev.insight.ui.InsightFragment"
        android:label="fragment_insight"
        tools:layout="@layout/fragment_insight" />

</navigation>